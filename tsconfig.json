{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "esnext",
    "target": "es6",
    "lib": ["es2019", "es6", "dom"],
    "sourceMap": false,
    "allowJs": false,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": true,
  }
}
